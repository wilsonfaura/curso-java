
public class Arrays_bidimensional {

	public static void main(String[] args) {
		int [][] matrix=new int[4][5];
		matrix[0][0]=15;
		matrix[0][1]=12;
		matrix[0][2]=9;
		matrix[0][3]=125;
		matrix[0][4]=135;
		
		matrix[1][0]=1;
		matrix[1][1]=22;
		matrix[1][2]=92;
		matrix[1][3]=35;
		matrix[1][4]=11;
		
		matrix[2][0]=5;
		matrix[2][1]=12;
		matrix[2][2]=9;
		matrix[2][3]=2;
		matrix[2][4]=65;
		
		matrix[3][0]=10;
		matrix[3][1]=7;
		matrix[3][2]=9;
		matrix[3][3]=11;
		matrix[3][4]=115;
		
		System.out.println("Valor almacenado en la posicion 2, 3 " + matrix[2][3]);

	}

}
